<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.three.kwctl.mykwlaunching.MainActivity">

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/btn_campus"
        android:onClick="onClick1"
        android:src="@drawable/campus"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        android:padding="0dp"
        android:layout_above="@+id/btn_library"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="45dp" /> //1번 유캠퍼스

    <ImageButton
        android:layout_width="110dp"
        android:layout_height="105dp"
        android:id="@+id/btn_notice"
        android:onClick="onClick2"
        android:src="@drawable/notice"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        android:padding="0dp"
        android:layout_centerHorizontal="true"
        android:layout_alignTop="@+id/btn_campus" />  //2번 공지사항

    <ImageButton
        android:layout_width="99dp"
        android:layout_height="100dp"
        android:id="@+id/btn_lunch"
        android:onClick="onClick3"
        android:src="@drawable/lunch"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        android:padding="0dp"
        android:layout_above="@+id/textView2"
        android:layout_alignLeft="@+id/btn_phone"
        android:layout_alignStart="@+id/btn_phone"
        android:layout_alignTop="@+id/btn_notice"
        android:contentDescription="lunch" />   //3번 식단

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/btn_library"
        android:onClick="onClick4"
        android:src="@drawable/library"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        android:padding="0dp"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:contentDescription="library" />   //4번 도서관

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/btn_shop"
        android:onClick="onClick5"
        android:src="@drawable/shop"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        android:padding="0dp"
        android:layout_alignTop="@+id/btn_library"
        android:layout_alignLeft="@+id/btn_facebook"
        android:layout_alignStart="@+id/btn_facebook" />   // 5번 주변 식당

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/btn_phone"
        android:onClick="onClick6"
        android:src="@drawable/phone"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        android:padding="0dp"
        android:layout_alignTop="@+id/btn_shop"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />   // 6번 전화번호


    <ImageButton
        android:layout_width="105dp"
        android:layout_height="100dp"
        android:id="@+id/btn_schedule"
        android:onClick="onClick7"
        android:src="@drawable/schedule"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        android:padding="0dp"
        android:layout_below="@+id/btn_library"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="43dp" />   // 7번 학사일정

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/btn_facebook"
        android:onClick="onClick8"
        android:src="@drawable/facebook"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        android:padding="0dp"
        android:layout_alignTop="@+id/btn_schedule"
        android:layout_centerHorizontal="true" />   //8번 페이스북

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/btn_card"
        android:onClick="onClick9"
        android:src="@drawable/card"
        android:background="#FFFFFF"
        android:scaleType="centerCrop"
        android:padding="0dp"
        android:layout_alignTop="@+id/btn_facebook"
        android:layout_alignLeft="@+id/btn_phone"
        android:layout_alignStart="@+id/btn_phone" />   //9번 학생증

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="U-Campus"
        android:gravity="center"
        android:id="@+id/textView"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/btn_notice"
        android:layout_alignRight="@+id/btn_campus"
        android:layout_alignEnd="@+id/btn_campus" />   //1번 유캠퍼스


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="공지사항"
        android:gravity="center"
        android:layout_below="@id/btn_notice"
        android:layout_alignRight="@id/btn_notice"
        android:layout_alignLeft="@id/btn_notice"
        android:id="@+id/textView2" />//2번 공지사항

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="학식"
        android:gravity="center"
        android:id="@+id/textView1"
        android:layout_alignLeft="@+id/btn_lunch"
        android:layout_alignStart="@+id/btn_lunch"
        android:layout_alignTop="@+id/textView2"
        android:layout_alignRight="@+id/btn_lunch"
        android:layout_alignEnd="@+id/btn_lunch"
        />
    //3번 학식


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="중앙도서관"
        android:gravity="center"
        android:layout_alignLeft="@+id/btn_library"
        android:layout_alignStart="@+id/btn_library"
        android:layout_alignRight="@+id/btn_library"
        android:layout_alignEnd="@+id/btn_library"
        android:layout_below="@+id/btn_library"
        android:layout_alignBottom="@+id/textView6" />      //4번 중앙도서관

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="주변 식당"
        android:gravity="center"
        android:layout_alignRight="@+id/btn_shop"
        android:layout_alignEnd="@+id/btn_shop"
        android:layout_alignTop="@+id/textView5"
        android:layout_alignLeft="@+id/btn_shop"
        android:layout_alignStart="@+id/btn_shop"
        android:id="@+id/textView6" />      //5번 주변식당

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="교내 전화"
        android:gravity="center"
        android:layout_below="@+id/btn_phone"
        android:layout_alignLeft="@+id/btn_phone"
        android:layout_alignStart="@+id/btn_phone"
        android:layout_alignRight="@+id/btn_phone"
        android:layout_alignEnd="@+id/btn_phone"
        android:id="@+id/textView5" />      //6번 전화번호

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="학사 일정"
        android:gravity="center"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/textView4"
        android:layout_alignRight="@+id/btn_schedule"
        android:layout_alignEnd="@+id/btn_schedule" />      //7번 학사일정

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="페이스북"
        android:gravity="center"
        android:id="@+id/textView4"
        android:layout_alignRight="@+id/btn_facebook"
        android:layout_alignEnd="@+id/btn_facebook"
        android:layout_below="@+id/btn_facebook"
        android:layout_alignLeft="@+id/btn_facebook"
        android:layout_alignStart="@+id/btn_facebook" />      //8번 페이스북


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="모바일 학생증 발급"
        android:gravity="center"
        android:id="@+id/textView3"
        android:layout_alignRight="@+id/btn_card"
        android:layout_alignEnd="@+id/btn_card"
        android:layout_below="@+id/btn_card"
        android:layout_alignLeft="@+id/btn_card"
        android:layout_alignStart="@+id/btn_card" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="KwangWoon University"
        android:id="@+id/textView7"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="false"
        android:layout_alignRight="@+id/btn_lunch"
        android:layout_alignEnd="@+id/btn_lunch"
        android:textAlignment="center"
        android:textColor="#3649b9" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전화하기"
        android:id="@+id/button4"
        android:layout_below="@+id/textView4"
        android:layout_centerHorizontal="true" />
    //9번 학생증
</RelativeLayout>
