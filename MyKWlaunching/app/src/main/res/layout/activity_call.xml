<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.three.kwctl.mykwlaunching.CallActivity">

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton31"
        android:onClick="onClick31"
        android:src="@drawable/subject"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/imageButton32"
        android:layout_alignStart="@+id/imageButton32" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton32"
        android:onClick="onClick32"
        android:src="@drawable/office"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <ImageButton
        android:layout_width="100dp"
    android:layout_height="100dp"
    android:id="@+id/imageButton33"
    android:onClick="onClick33"
    android:src="@drawable/other"
    android:layout_alignParentBottom="true"
    android:layout_alignLeft="@+id/textView9"
    android:layout_alignStart="@+id/textView9" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="과 사무실"
        android:id="@+id/textView8"
        android:gravity="center"
        android:layout_alignRight="@+id/imageButton31"
        android:layout_alignEnd="@+id/imageButton31"
        android:layout_below="@+id/imageButton31"
        android:layout_alignLeft="@+id/imageButton31"
        android:layout_alignStart="@+id/imageButton31" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="사무처"
        android:gravity="center"
        android:id="@+id/textView9"
        android:layout_alignRight="@+id/imageButton32"
        android:layout_alignEnd="@+id/imageButton32"
        android:layout_below="@+id/imageButton32"
        android:layout_alignLeft="@+id/imageButton32"
        android:layout_alignStart="@+id/imageButton32" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="기타"
        android:gravity="center"
        android:id="@+id/textView10"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/imageButton33"
        android:layout_alignEnd="@+id/imageButton33"
        android:layout_alignLeft="@+id/imageButton33"
        android:layout_alignStart="@+id/imageButton33"
        />


</RelativeLayout>
